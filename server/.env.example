# ElevenLabs TTS Streaming Server Configuration
# Copy this file to .env and fill in your values

# ===========================================
# OpenAI Configuration (for LLM/STT)
# ===========================================
OPENAI_API_KEY=sk-your-openai-api-key-here

# ===========================================
# ElevenLabs Configuration
# ===========================================
ELEVEN_API_KEY=your-elevenlabs-api-key-here
ELEVEN_VOICE_ID=your-voice-id-here
ELEVEN_MODEL=eleven_flash_v2_5

# Optimize streaming latency (0-4, higher = faster but less normalization)
ELEVEN_OPTIMIZE_LATENCY=1

# ===========================================
# LiveKit Configuration
# ===========================================
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# ===========================================
# Server Configuration
# ===========================================
# Token server port (for LiveKit tokens)
PORT=8080

# TTS WebSocket server port
TTS_WS_PORT=8081

# TTS WebSocket URL (used by agent to send LLM responses)
TTS_WS_URL=ws://localhost:8081/stream_tts

# ===========================================
# Logging Configuration
# ===========================================
LOG_LEVEL=INFO

# Save alignment data to files (true/false)
LOG_ALIGNMENT=true

# Save raw text outputs to files (true/false)
LOG_OUTPUTS=true
